{
  "name": "Skookum",
  "description": "An amazing, rich, isomorphic website created with skookum-react",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "iojs": "2.0.x"
  },
  "scripts": {
    "lint": "npm run eslint && npm run suit",
    "eslint": "eslint .",
    "suit": "node scripts/suit-conformance",
    "start": "DEBUG=*,-babel node . & node server.js",
    "test": "jest",
    "build": "node ./scripts/build && node dist/main.js"
  },
  "dependencies": {
    "array.prototype.find": "1.0.0",
    "autoprefixer-loader": "2.0.0",
    "babel-core": "5.4.7",
    "babel-eslint": "5.0.0",
    "babel-jest": "5.2.0",
    "babel-loader": "5.1.3",
    "babel-runtime": "6.6.1",
    "body-parser": "1.12.4",
    "btoa": "1.1.2",
    "classnames": "^2.2.5",
    "contentful": "1.1.1",
    "css-loader": "0.14.4",
    "debug": "2.2.0",
    "es6-promise": "2.1.1",
    "escape-html": "1.0.1",
    "esprima-loader": "0.9.0",
    "express": "4.12.4",
    "extract-text-webpack-plugin": "0.8.1",
    "file-loader": "0.8.4",
    "flux": "2.0.3",
    "glob": "5.0.10",
    "immutable": "3.7.3",
    "isomorphic-fetch": "2.1.0",
    "marked": "0.3.5",
    "node-noop": "0.0.1",
    "normalize.css": "3.0.3",
    "object.assign": "3.0.0",
    "react": "0.13.3",
    "react-cookie": "0.4.2",
    "react-doc-meta": "0.2.0",
    "react-resolver": "1.3.0",
    "react-router": "0.13.3",
    "react-tabs": "iamdustan/react-tabs#33ce42ed79059c24aca6cb25aaa5bc2d47173e9b",
    "react-tap-event-plugin": "0.1.7",
    "react-tween-state": "0.0.5",
    "rework": "1.0.1",
    "rework-vars": "3.1.1",
    "rework-webpack-loader": "0.2.0",
    "source-map": "0.4.2",
    "source-map-support": "0.3.1",
    "style-loader": "0.12.3",
    "suitcss-preprocessor": "0.4.0",
    "url-loader": "0.5.6",
    "node-libs-browser": "<=0.6.0",
    "webpack": "1.9.10"
  },
  "devDependencies": {
    "chalk": "1.0.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.14.0",
    "jest-cli": "0.4.5",
    "node-gyp": "^3.5.0",
    "react-hot-loader": "1.2.7",
    "rework-suit-conformance": "0.4.2",
    "webpack-dev-server": "1.9.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest-preprocessor",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "js"
    ]
  }
}
